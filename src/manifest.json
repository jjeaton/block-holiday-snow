{
   "name": "Block Holiday Snow",
   "version": "0.1",
   "manifest_version": 2,
   "description": "Blocks the snow from falling on any site using the WordPress Jetpack plugin.",
   "icons": { "16": "icons/16x16.png", "48": "icons/48x48.png", "128": "icons/128x128.png" },
   "background": {
       "scripts": ["background.ja"]
   },
   "permissions": [
       "webRequest",
       "webRequestBlocking",
       "<all_urls>"
   ]
}
